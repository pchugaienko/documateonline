{
    "name": "pchugaienko/diia-sdk",
    "description": "SDK for Diia API integration",
    "version": "1.0.0",
    "require": {
        "php": "^7.2 || ^8.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
